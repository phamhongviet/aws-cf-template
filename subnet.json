{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {
        "MySubnet": {
            "Properties": {
                "AvailabilityZone": "us-east-1a",
                "CidrBlock": "172.16.128.0/24",
                "MapPublicIpOnLaunch": "false",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "MySubnet"
                    }
                ],
                "VpcId": {
                    "Ref": "MyExampleVpc"
                }
            },
            "Type": "AWS::EC2::Subnet"
        }
    }
}
